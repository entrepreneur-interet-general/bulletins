{
    "name":"entrepreneur-interet-general/bulletins",
    "type":"project",
    "description":"Bulletins is a simple weekly retrospective tool for multiple projects or teams",
    "keywords":[
        "project-management",
        "reporting",
        "reports",
        "retrospective",
        "standup",
        "archive",
        "bulletin",
        "bulletins",
        "laravel"
    ],
    "license":"AGPL-3.0",
    "authors":[
        {
            "name":"Antoine Augusti",
            "email":"antoine.augusti@data.gouv.fr",
            "homepage":"https://www.antoine-augusti.fr",
            "role":"Developer"
        }
    ],
    "support":{
        "issues":"https://github.com/entrepreneur-interet-general/bulletins/issues"
    },
    "require":{
        "php":"^7.1.3",
        "bugsnag/bugsnag-laravel":"^2.0",
        "fideloper/proxy":"^4.0",
        "laravel/framework":"5.8.*",
        "laravel/tinker":"^1.0",
        "wrapi/slack":"^1.0"
    },
    "require-dev": {
        "themsaid/laravel-langman": "^1.3",
        "beyondcode/laravel-dump-server": "^1.0",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.5"
    },
    "config":{
        "optimize-autoloader":true,
        "preferred-install":"dist",
        "sort-packages":true
    },
    "extra":{
        "laravel":{
            "dont-discover":[

            ]
        }
    },
    "autoload":{
        "psr-4":{
            "App\\":"app/"
        },
        "classmap":[
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev":{
        "psr-4":{
            "Tests\\":"tests/"
        }
    },
    "minimum-stability":"dev",
    "prefer-stable":true,
    "scripts":{
        "post-autoload-dump":[
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install":[
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd":[
            "@php artisan key:generate --ansi"
        ]
    }
}
